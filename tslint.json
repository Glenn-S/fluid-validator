{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-line-length": {
      "options": [100],
      "secerity": "warning"
    },
    "no-console": {
      "severity": "error",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  }
}
